{
  "name": "ccn-toolset",
  "version": "1.0.0",
  "description": "CCN file tools",
  "main": "first.js",
  "bin": "dist/bin.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "bin": "node --max-old-space-size=4096 dist/bin.js",
    "finder": "node dist/bin.js finder",
    "avg10m": "node --max-old-space-size=4096 dist/bin.js avg10m",
    "avg1m": "node --max-old-space-size=4096 dist/bin.js avg1m",
    "avg10m-folder": "node --max-old-space-size=8192 dist/bin.js avg10m-folder",
    "slice-file": "node dist/bin.js slice-file",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "csv-parse": "^4.15.0",
    "exceljs": "^4.2.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "typescript": "^4.1.3",
    "@types/node": "^10.16.0"
  }
}
